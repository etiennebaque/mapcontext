<%= javascript_include_tag 'bootstrap.min' %>

<!-- library for cookie management -->
<%= javascript_include_tag 'jquery.cookie' %>
<!-- calender plugin -->
<%= javascript_include_tag 'moment.min' %>
<%= javascript_include_tag 'fullcalendar.min' %>
<!-- data table plugin -->
<%= javascript_include_tag 'jquery.dataTables.min' %>

<!-- select or dropdown enhancer -->
<%= javascript_include_tag 'chosen.jquery.min' %>
<!-- plugin for gallery image view -->
<%= javascript_include_tag 'jquery.colorbox-min' %>
<!-- notification plugin -->
<%= javascript_include_tag 'jquery.noty' %>
<!-- library for making tables responsive -->
<%= javascript_include_tag 'responsive-tables' %>
<!-- tour plugin -->
<%= javascript_include_tag 'bootstrap-tour.min' %>
<!-- star rating plugin -->
<%= javascript_include_tag 'jquery.raty.min' %>
<!-- for iOS style toggle switch -->
<%= javascript_include_tag 'jquery.iphone.toggle' %>
<!-- autogrowing textarea plugin -->
<%= javascript_include_tag 'jquery.autogrow-textarea' %>
<!-- multiple file upload plugin -->
<%= javascript_include_tag 'jquery.uploadify-3.1.min' %>
<!-- history.js for cross-browser state change on ajax -->
<%= javascript_include_tag 'jquery.history' %>
<!-- application script for Charisma demo -->
<%= javascript_include_tag 'charisma' %>

<%= javascript_include_tag 'bootstrap-wysiwyg' %>

<%= javascript_include_tag 'general' %>

<%= javascript_include_tag 'map_settings' %>

<!-- jquery_ujs is already included in application.js. However, some Bootstrap themes might override it, causing
 functionalities like Devise sign out no to work. Therefore, we include jquery_ujs after loading all scripts -->
<%= javascript_include_tag 'jquery_ujs' %>